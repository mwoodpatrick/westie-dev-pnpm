{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.10.7/.schema/devbox.schema.json",
  "packages": [
    "nodejs@latest",
    "nodePackages_latest.pnpm@latest",
    "go@latest",
    "neovim@latest",
    "delve@latest",
    "mkcert@latest",
    "direnv@latest",
    "git@latest",
    "gh@latest",
    "firefox@latest",
    "nss@latest",
    "google-chrome@latest",
    "gox@latest",
    "rustup@latest",
    "libiconv@latest",
    "neovide@latest",
    "oh-my-zsh@latest",
    "tree@2.1.3",
    "zsh@latest",
    "lua@latest",
    "eza@latest",
    "bat@latest"
  ],
  "shell": {
    "init_hook": [
      "echo 'Welcome to devbox!'",
      "gh auth login --with-token < ~/my_github_token.txt",
      "gh auth setup-git",
      "gh auth status",
      "zsh"
    ],
    "scripts": {
      "test": [
        "echo \"Error: no test specified\" && exit 1"
      ]
    }
  }
}

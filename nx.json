{
  "namedInputs": {
    "noMarkdown": ["!{projectRoot}/**/*.md"],
    "production": ["default", "!{projectRoot}/src/test-setup.[jt]s"]
  },
  "targetDefaults": {
    "build": {
      "inputs": ["noMarkdown", "^noMarkdown"],
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": ["^build", "pre-build"],
      "cache": true
    },
    "dev": {
      "dependsOn": ["^build"]
    },
    "@nx/jest:jest": {
      "inputs": ["noMarkdown", "{workspaceRoot}/jest.preset.js"],
      "cache": true,
      "options": {
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "@nx/eslint:lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"],
      "cache": true
    },
    "@nx/esbuild:esbuild": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"]
    }
  },
  "nxCloudAccessToken": "Mzk5NzRjODQtNThkYS00OWJhLWJmNjEtYmQwNDVjNDUzODhkfHJlYWQtd3JpdGU=",
  "useInferencePlugins": false
}
